<?xml version="1.0" encoding="UTF-8"?>
<!-- Echo: Decaying repetition effect with pitch shift -->
<spa xmlns="https://spa.audio/ns" version="1.0" xmlns="http://spa.audio/ns">
  <tone wave="sine" freq="440" dur="0.2"
        repeat="8"
        repeat.interval="0.15"
        repeat.decay="0.3"
        repeat.pitchShift="-2"
        amp="0.8"
        envelope="0.01,0.05,0.5,0.1"/>
</spa>