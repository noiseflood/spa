<?xml version="1.0" encoding="UTF-8"?>
<!-- Alarm: Alternating tones with repeat for urgent notification -->
<spa version="1.0" xmlns="http://spa.audio/ns">
  <group repeat="5" repeat.interval="0.1">
    <tone wave="square" freq="880" dur="0.15"
          amp="0.7"
          envelope="0,0.05,0.8,0.05"/>
    <tone wave="square" freq="660" dur="0.15"
          amp="0.7"
          envelope="0,0.05,0.8,0.05"
          repeat="1" repeat.interval="0.15"/>
  </group>
</spa>