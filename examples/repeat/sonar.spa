<?xml version="1.1" encoding="UTF-8"?>
<!-- Sonar Ping: Classic submarine sonar with echo decay -->
<spa xmlns="https://spa.audio/ns" version="1.1">
  <tone wave="sine" freq="1500" dur="0.1"
        repeat="3"
        repeat.interval="1.0"
        repeat.decay="0.5"
        amp="0.8"
        envelope="0,0.1,0.8,0.3"
        filter="bandpass" cutoff="1500" resonance="5"/>
</spa>