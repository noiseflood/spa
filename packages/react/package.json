{
  "name": "@spa/react",
  "version": "1.0.0",
  "description": "React component for SPA (Scalable Parametric Audio) - procedural sound effects in React apps",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
