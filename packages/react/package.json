    "@types/react": "^18.2.0",
    "jest": "^29.5.0",
    "react": "^18.2.0",
    "rollup": "^3.20.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/spa"
  },
  "keywords": [
